<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>RongCloud.Server</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>HeMiao</Authors>
    <Company>shanda.com</Company>
    <Product>DecodeCamp</Product>
    <PackageReleaseNotes>
      rongcloud sdk for commit
      https://github.com/rongcloud/server-sdk-dotnet/tree/187b0c945b2df5bdabd38d7437c3ecdc05f4bbe9
    </PackageReleaseNotes>
    <Version>5.2019.6.14</Version>
    <AssemblyVersion>5.2019.6.14</AssemblyVersion>
    <FileVersion>5.2019.6.14</FileVersion>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>融云的SDK打包成nuget</Description>
    <RepositoryUrl>https://github.com/rongcloud/server-sdk-dotnet</RepositoryUrl>
    <PackageIcon>favicon.png</PackageIcon>
    <PackageProjectUrl>https://www.rongcloud.cn/docs/server.html</PackageProjectUrl>
    <RepositoryType>github</RepositoryType>
    <Copyright>RongCloud</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="jsonsource/**/*.*">
      <IncludeInPackage>true</IncludeInPackage>
      <PackageCopyToOutput>true</PackageCopyToOutput>
      <PackageFlatten>false</PackageFlatten>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <None Include="favicon.png" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>